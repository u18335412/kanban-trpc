{
  "name": "@examples/next-starter",
  "version": "9.25.3",
  "private": true,
  "scripts": {
    "build:1-migrate": "prisma migrate deploy",
    "build:2-build": "doppler run next build",
    "build": "doppler run run-s build:*",
    "db-up": "docker-compose up -d",
    "db-seed": "prisma db seed",
    "db-migrate-dev": "npx prisma migrate dev",
    "db-nuke": "docker-compose down --volumes --remove-orphans",
    "dev": "doppler run -- next dev",
    "dx": "run-s db-up db-migrate-dev db-seed dev",
    "start": "doppler run next start",
    "lint": "eslint src",
    "lint-fix": "npx lint --fix",
    "test-dev": "start-server-and-test dev 3000 test",
    "test-start": "start-server-and-test start 3000 test",
    "test": "run-s test:*",
    "test:unit": "jest",
    "test:e2e": "playwright test",
    "ts-node": "ts-node --compiler-options \"{\\\"module\\\":\\\"commonjs\\\"}\"",
    "postinstall": "prisma generate"
  },
  "prisma": {
    "seed": "doppler run npx ts-node prisma/seed.js"
  },
  "prettier": {
    "printWidth": 80,
    "trailingComma": "all",
